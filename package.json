{
  "name": "vue-app",
  "type": "module",
  "version": "1.0.0",
  "private": true,
  "packageManager": "pnpm@10.17.1",
  "engines": {
    "node": "^20.19.0 || >=22.12.0"
  },
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "build-only": "vite-ssg build",
    "type-check": "vue-tsc --build",
    "lint": "eslint . --fix",
    "format": "prettier -w *.ts src",
    "start": "pnpm lint && pnpm format && cls && pnpm dev"
  },
  "dependencies": {
    "@unhead/vue": "^2.0.17",
    "@unocss/preset-icons": "latest",
    "@unocss/preset-wind4": "latest",
    "@vueuse/core": "latest",
    "jspdf": "^3.0.3",
    "jspdf-autotable": "^5.0.2",
    "unocss": "latest",
    "vue": "^3.5.18",
    "vue-router": "^4.5.1"
  },
  "devDependencies": {
    "@iconify-json/uil": "^1.2.3",
    "@nirtamir2/prettier-plugin-css-order": "^0.0.1",
    "@tsconfig/node22": "^22.0.2",
    "@types/node": "^22.16.5",
    "@unocss/eslint-config": "^66.5.2",
    "@unocss/eslint-plugin": "^66.5.2",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.6.0",
    "@vue/tsconfig": "^0.7.0",
    "eslint": "^9.36.0",
    "eslint-plugin-vue": "^10.5.0",
    "fontless": "^0.0.2",
    "jiti": "^2.6.0",
    "npm-run-all2": "^8.0.4",
    "prettier": "^3.6.2",
    "typescript": "~5.8.0",
    "unplugin-vue-router": "^0.15.0",
    "vite": "npm:rolldown-vite@latest",
    "vite-plugin-vue-devtools": "^8.0.0",
    "vite-ssg": "^28.1.0",
    "vite-ssg-sitemap": "^0.10.0",
    "vue-tsc": "^3.0.4"
  }
}
