    "start": "pnpm lint && pnpm format && clear && pnpm dev"
  },
  "dependencies": {
    "@unhead/vue": "^2.0.19",
    "@vueuse/core": "^14.0.0",
    "jspdf": "^3.0.3",
    "jspdf-autotable": "^5.0.2",
    "unocss": "66.5.4",
    "vue": "^3.5.22",
    "vue-router": "^4.6.3"
  },
  "devDependencies": {
    "@iconify-json/uil": "^1.2.3",
    "@tsconfig/node22": "^22.0.2",
    "@types/node": "^22.16.5",
    "@unocss/eslint-config": "latest",
    "@unocss/eslint-plugin": "latest",
    "@unocss/preset-wind4": "^66.5.4",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.6.0",
    "@vue/tsconfig": "^0.8.1",
    "eslint": "^9.38.0",
    "eslint-plugin-vue": "^10.5.1",
    "fontless": "^0.0.2",
