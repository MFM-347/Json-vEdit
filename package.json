    "typescript": "~5.9.0",
    "unplugin-auto-import": "^20.2.0",
    "unplugin-icons": "^22.5.0",
    "unplugin-vue-components": "^30.0.0",
    "unplugin-vue-router": "^0.16.0",
    "vite": "npm:rolldown-vite@latest",
    "vite-plugin-vue-devtools": "^8.0.3",
    "vite-ssg": "^28.2.2",
    "vite-ssg-sitemap": "^0.10.0",
    "vue-tsc": "^3.1.1"
